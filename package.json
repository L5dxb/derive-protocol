    "request": "^2.88.0",
    "solc": "0.5.5",
    "tabookey-gasless": "^0.4.0",
    "truffle": "5.0.13",
    "truffle-hdwallet-provider-privkey": "^1.0.3",
    "truffle-security": "^1.4.2",
    "web3": "1.0.0-beta.36"
  },
  "repository": {
